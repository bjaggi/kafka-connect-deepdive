{
"name": "postgres-cdc-azure-src",
"config": {
"connector.class": "io.debezium.connector.postgresql.PostgresConnector",
"tasks.max": "1",
"database.hostname": "ccapppg.postgres.database.azure.com",
"database.port": "5432",
"database.user": "pgadm@ccapppg",
"config.providers": "file",
"config.providers.file.class": "org.apache.kafka.common.config.provider.FileConfigProvider",
"connection.password": "${file:/mnt/secrets/test-db-pass/my_credentials:password}",
"database.dbname" : "ccapp",
"database.server.name": "ccapppg",
"database.history.kafka.bootstrap.servers": "pkc-ldvmy.centralus.azure.confluent.cloud:9092",
"database.history.kafka.topic": "schema-changes.inventory",
"table.include.list": "ccappadmin.visit",
"plugin.name": "wal2json",
"value.converter": "io.confluent.connect.avro.AvroConverter",
"key.converter": "org.apache.kafka.connect.storage.StringConverter",
"schema.registry.url": "https://psrc-gqpv.westus2.azure.confluent.cloud",
"value.converter.schema.registry.url": "https://psrc-gq7pv.westus2.azure.confluent.cloud",
"value.converter.basic.auth.credentials.source": "USER_INFO",
"value.converter.schema.registry.basic.auth.user.info": "SQWLHTFDXRLBZWIG:7E6oySTKmTegGDJ3uAq/KjG5gYhUsT7oH/eNSXr2DzoivjIlrSDWTEUlr55xsSJJ",
"transforms": "unwrap, filterPostGres, ExtractField, TimestampConverter",
"transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
"transforms.unwrap.add.headers":  "op,source.schema",
"transforms.unwrap.delete.handling.mode": "none",
"transforms.unwrap.drop.tombstones": "true",
"transforms.filterPostGres.type": "io.confluent.connect.transforms.Filter$Value",
"transforms.filterPostGres.filter.condition": "$[?(@.source == \"ONPREM\")]",
"transforms.filterPostGres.filter.type": "exclude",
"transforms.filterPostGres.missing.or.null.behavior": "include",
"transforms.TimestampConverter.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
"transforms.TimestampConverter.field": "date_created",
"transforms.TimestampConverter.format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'",
"transforms.TimestampConverter.target.type": "Timestamp",
"transforms.ExtractField.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
"transforms.ExtractField.field": "visit_id",
"time.precision.mode": "adaptive_time_microseconds",
"errors.tolerance": "all",
"errors.deadletterqueue.topic.name": "ccapppg.ccappadmin.visit.error",
"errors.log.enable": "true",
"errors.log.include.messages": "true"
}
